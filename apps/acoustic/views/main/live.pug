extends layout

block header_meta
	- var content_title= 'acoustic | treatment / live'
	- var menu_active = 'live'

block header_include
	- var include_css = ['live.css', 'content.css']
	- var include_js = ['live.js']

block content
	.panel_block.left: .panel_inner
		each event in events
			.event_item(class=event.type)
				.event_num= event.numb
				a.event_title(href='#' + event._short_id)= event.title
				.event_type: span= event.type

	.panel_block.right: .panel_inner